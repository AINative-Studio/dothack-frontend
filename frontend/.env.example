# ZeroDB Configuration
# Copy this file to .env.local and fill in your actual values

# ZeroDB API Base URL
NEXT_PUBLIC_ZERODB_API_BASE=https://api.zerodb.io

# Your ZeroDB Project UUID
# Get this from your ZeroDB dashboard or project creation response
NEXT_PUBLIC_ZERODB_PROJECT_ID=your-project-uuid-here

# Your ZeroDB API Key
# Get this from your ZeroDB dashboard
# Keep this secret - never commit to git!
NEXT_PUBLIC_ZERODB_API_KEY=your-api-key-here

# Authentication Configuration
NEXT_PUBLIC_AINATIVE_AUTH_URL=https://api.ainative.studio/v1/auth
NEXT_PUBLIC_AINATIVE_APP_URL=http://localhost:3000

# Optional: WebSocket Configuration (for future use)
# NEXT_PUBLIC_WS_URL=ws://localhost:8080
# NEXT_PUBLIC_WS_URL_PRODUCTION=wss://leaderboard.dothack.com
